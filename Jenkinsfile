pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('develop') {
            steps {
                echo 'develop the program'
            }
        }
        stage('test') {
            steps {
                echo 'test the program'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy the program'
            }
        }
    }
}
